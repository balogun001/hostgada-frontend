{
  "tailwindCSS.codeActions": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tailwindCSS.experimental.classRegex": [
    [
      "cva\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]",
      "classnames\\(([^)]*)\\)",
      "'([^']*)'"
    ]
  ],
  "cSpell.words": [
    "Abideen",
    "Akanni",
    "Ekiti",
    "heroicons",
    "hookform",
    "longcode",
    "RECAPTCHA",
    "Renderable",
    "sitekey",
    "stackings",
    "Subaccount"
  ]
}
